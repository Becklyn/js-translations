{
    "name": "becklyn/translations",
    "type": "symfony-bundle",
    "description": "A bundle that extracts translations",
    "homepage": "https://github.com/Becklyn/translations-bundle",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/config": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "symfony/http-kernel": "^4.3",
        "symfony/translation-contracts": "^1.1"
    },
    "require-dev": {
        "becklyn/php-cs": "^1.4"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\Translations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\Translations\\": "tests/"
        }
    }
}
