{
    "name": "becklyn/translations",
    "type": "symfony-bundle",
    "description": "A bundle that extracts translations",
    "homepage": "https://github.com/Becklyn/translations-bundle",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "https://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "symfony/config": "^4.3 || ^5.0",
        "symfony/dependency-injection": "^4.3 || ^5.0",
        "symfony/framework-bundle": "^4.3 || ^5.0",
        "symfony/http-kernel": "^4.3 || ^5.0",
        "symfony/process": "^4.3 || ^5.0",
        "symfony/translation": "^4.3 || ^5.0",
        "symfony/translation-contracts": "^1.1 || ^2.0",
        "twig/twig": "^2.11 || ^3.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-next": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\Translations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\Translations\\": "tests/"
        }
    }
}
